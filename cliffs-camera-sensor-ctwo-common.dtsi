&L7E {
	regulator-min-microvolt = <1800000>;
	regulator-max-microvolt = <1800000>;
	qcom,init-voltage = <1800000>;
};

&pmxr2230_flash0 {
	qcom,max-current-ma = <650>;
	qcom,duration-ms = <500>;
};
&pmxr2230_flash1 {
	qcom,max-current-ma = <650>;
	qcom,duration-ms = <500>;
};
&pmxr2230_flash2 {
	qcom,duration-ms = <500>;
};
&pmxr2230_flash3 {
	qcom,duration-ms = <500>;
};

&pmxr2230_torch0 {
	qcom,max-current-ma = <125>;
};

&pmxr2230_torch1 {
	qcom,max-current-ma = <125>;
};

&soc {
	cam_dvdd_uw: gpio-regulator@0 {
